{
	"source": {
		"include": [
			"src/"
		]
	},
	"opts": {
		"encoding": "utf8",
		"readme": "README.md",
		"recurse": true,
		"template": "node_modules/jsdoc-wmf-theme",
		"package": "package.json",
		"destination": "docs"
	},
	"plugins": [
		"plugins/markdown",
		"plugins/summarize"
	],
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		},
		"wmf": {
			"maintitle": "DOM Word Replacer",
			"repository": "https://github.com/mooeypoo/dom-word-replacer",
			"linkMap": {
				"@todo": "Some of these can be removed after https://github.com/jsdoc/jsdoc/issues/1585",
				"Array": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array",
				"Error": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Error",
				"HTMLElement": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/HTMLElement",
        "Object": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Object",
        "Document": "https://developer.mozilla.org/en-US/docs/Web/API/Document"
			}
		}
	}
}